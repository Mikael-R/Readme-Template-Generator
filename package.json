{
  "name": "readme-template-generator",
  "version": "0.4.0",
  "description": "CLI to help create readme file to document your project",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mikael-R/readme-template-generator.git"
  },
  "keywords": [
    "generator",
    "cli",
    "readme",
    "readme-template",
    "readme-template-generator"
  ],
  "author": "Mikael Rolim de Aquino <mikarg9@gmail.com>",
  "bugs": {
    "url": "https://github.com/Mikael-R/readme-template-generator/issues"
  },
  "homepage": "https://github.com/Mikael-R/readme-template-generator#readme",
  "types": "build/types/types.d.ts",
  "bin": {
    "readme-template-generator": "bin/readme-template-generator"
  },
  "scripts": {
    "format": "eslint src --ext ts",
    "build": "rm -rf build && babel src --extensions \".ts\" --out-dir build --copy-files --no-comments",
    "prepublishOnly": "npm run format && npm run build",
    "test": "jest",
    "watch": "jest --watch",
    "snapupdate": "jest --updateSnapshot",
    "coverage": "jest --coverage"
  },
  "files": [
    "tsconfig.json",
    "build",
    "LICENSE",
    "README.md",
    "docs",
    "bin"
  ],
  "license": "MIT",
  "dependencies": {
    "cfonts": "^2.8.5",
    "gluegun": "latest",
    "inquirer": "^7.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/node": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-conventional": "^9.0.1",
    "@lenne.tech/cli-plugin-helper": "0.0.8",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.11",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "3.2.0",
    "eslint": "^7.5.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "find-file-up": "^2.0.1",
    "git-cz": "^4.7.0",
    "husky": "^4.2.5",
    "jest": "^24.1.0",
    "standard": "^12.0.1",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "standard": {
    "env": [
      "jest"
    ]
  }
}
